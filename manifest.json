{
  "manifest_version": 2,
  "name": "My Extension",
  "description": "Hi Chrome",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/script.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": ["/background.js"],
    "persistent": false
  }
}
